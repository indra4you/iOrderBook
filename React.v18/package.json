{
    "name": "iorder-book",
    "version": "2.0.0",
    "private": true,
    "scripts": {
        "clean": "rm -rf ./publish",
        "postbuild": "node ./parcel.postbuild.js",
        "start": "parcel src/index.html",
        "build": "npm run clean && parcel build src/index.html --dist-dir ./publish --public-url https://indra4you.github.io/iOrderBook/ --no-source-maps && npm run postbuild"
    },
    "dependencies": {
        "bootstrap": "^5.3.3",
        "bootstrap-icons": "^1.11.3",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.53.0",
        "react-router": "^6.26.2",
        "react-router-dom": "^6.26.2"
    },
    "devDependencies": {
        "@parcel/transformer-sass": "^2.12.0",
        "@types/node": "^22.5.4",
        "@types/react": "^18.3.8",
        "@types/react-dom": "^18.3.0",
        "@types/react-router-dom": "^5.3.3",
        "@types/wicg-file-system-access": "^2023.10.5",
        "parcel": "^2.12.0",
        "process": "^0.11.10",
        "typescript": "^5.5.4"
    }
}